{
  "name": "koa-extended-server",
  "version": "1.0.1",
  "description": " An extended server for Koa",
  "main": "lib/server.js",
  "types": "lib/server.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyndonbuckley/koa-extended-server.git"
  },
  "keywords": [
    "node",
    "koa",
    "server"
  ],
  "author": "Lyndon Buckley <npm@lyndonbuckley.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/lyndonbuckley/koa-extended-server/issues"
  },
  "homepage": "https://github.com/lyndonbuckley/koa-extended-server#readme",
  "devDependencies": {
    "@types/node": "^13.13.2",
    "prettier": "^2.0.5",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "@types/koa": "^2.11.6"
  },
  "dependencies": {
    "koa": "^2.13.0"
  }
}
